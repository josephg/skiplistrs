    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'skiplistrs'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=skiplistrs"
                ],
                "filter": {
                    "name": "skiplistrs",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug integration test 'test'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--test=test",
                    "--package=skiplistrs"
                ],
                "filter": {
                    "name": "test",
                    "kind": "test"
                }
            },
            "args": ["--nocapture", "test::simple_edits"],
            "cwd": "${workspaceFolder}"
        }
    ]
}